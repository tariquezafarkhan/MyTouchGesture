<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ata.touchtest">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserIdentityActivity" android:label="UserIdentity"> </activity>
        <activity android:name=".TouchScreenActivity" android:label="TouchScreen"> </activity>
        <activity android:name=".ShowUserDataActivity" android:label="ShowScreen"> </activity>
        <activity android:name=".VerifyScreenActivity" android:label="VerifyUser"> </activity>
        <activity android:name=".ExportDataActivity" android:label="ExportData"> </activity>
        <activity android:name=".ClassifierActivity" android:label="Classifier"> </activity>
        <activity android:name=".GestureBuilderActivity" android:label="@string/application_name" > </activity>
        <activity android:name=".CreateGestureActivity" android:label="@string/label_create_gesture" > </activity>
        <activity android:name=".GestureTest" android:label="@string/verify_class" > </activity>
    </application>

</manifest>